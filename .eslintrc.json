{
  "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:jsx-a11y/recommended",
      "prettier",
      "next/core-web-vitals"
  ],
  "rules": {
      "react/prop-types": 0,
      "react/react-in-tsx-scope": 0
  },
  "plugins": ["react","import","jsx-a11y"],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module",
    "ecmaFeatures": {
      "tsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "settings": {
      "react": {
          "version": "detect"
      },
      "import/resolver": {
          "node": {
              "extensions": [".js", ".jsx", ".ts", ".tsx"]
          }
      }
  }
}
